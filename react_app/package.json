    "babel-register": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "expect": "^22.4.3",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.4",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "reactstrap": "^5.0.0-beta.3",
